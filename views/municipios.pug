extends layout

block content
  h1 Municipios de Venezuela
  
  
  table(id="municipios" class="display" style="width:80%")
    thead
      tr
        th ID
        th Municipio
        th Ciudad
        th Estado
        th iso

    tbody
      each municipio in municipios
        tr
          td #{municipio.id}
          td #{municipio.municipio}
          td #{municipio.Ciudad.ciudad}
          td #{municipio.Estado.estado}
          td #{municipio.Estado.iso_3166_2}

  a(href="/") Volver