extends layout

block content
  h1 Parroquias de Venezuela
  
  hr

  table(id="parroquias" class="display" style="width:80%")
    thead
      tr
        th ID
        th Parroquia
        th Municipio

    tbody
      each parroquia in parroquias
        tr
          td #{parroquia.id}
          td #{parroquia.parroquia}
          td #{parroquia.Municipio.municipio}

  a(href="/") Volver